# Google Apps Script Configuration
# copy this file to .env and add your script URL below
VITE_GOOGLE_APPS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID_HERE/exec

# Storyblok CMS Configuration
# Get your token from: Storyblok Dashboard > Settings > Access Tokens
# Use PREVIEW token for development (shows draft content)
# Use PUBLIC token for production (shows published content only)
VITE_STORYBLOK_ACCESS_TOKEN=your_storyblok_preview_token_here

# Storyblok Region (Optional)
# Set to "us" if your Storyblok space is in US region, defaults to "eu"
# VITE_STORYBLOK_REGION=eu

# HTTPS Configuration (Optional)
# Set to 'true' to enable HTTPS for Storyblok Visual Editor
# Note: First-time setup requires sudo password to install SSL certificate
# Leave empty or set to 'false' for regular HTTP development
# VITE_HTTPS=true

STORYBLOK_MANAGEMENT_TOKEN=token-here-please # The token will expire on Thu, 19/02/2026

